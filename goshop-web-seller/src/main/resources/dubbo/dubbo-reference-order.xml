                     interface="org.goshop.order.i.StoreCartService" check="false" />
    <dubbo:reference id="orderFormService"
                     interface="org.goshop.order.i.OrderFormService" check="false" />
    <dubbo:reference id="orderAddressService"
                     interface="org.goshop.order.i.OrderAddressService" check="false" />
    <dubbo:reference id="orderFormLogService"
                     interface="org.goshop.order.i.OrderFormLogService" check="false" />
    <dubbo:reference id="couponService"
                     interface="org.goshop.order.i.CouponService" check="false" />
